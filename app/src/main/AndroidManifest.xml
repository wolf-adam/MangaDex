<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mangadex">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/octo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/octo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.mangadex.ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mangadex.ui.character.CharacterActivity">
        </activity>
    </application>

</manifest>